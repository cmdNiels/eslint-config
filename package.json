{
	"name": "@cmdniels/eslint-config",
	"version": "1.2.0",
	"private": false,
	"description": "Opinionated ESLint configs for TypeScript and web (React/Next.js/Tailwind) projects",
	"keywords": [
		"eslint",
		"eslint-config",
		"prettier"
	],
	"homepage": "https://github.com/cmdniels/eslint-config",
	"repository": {
		"type": "git",
		"url": "https://github.com/cmdniels/eslint-config.git"
	},
	"license": "MIT",
	"author": "cmdNiels",
	"type": "module",
	"exports": {
		"./ts": "./ts.eslint.config.mjs",
		"./web": "./web.eslint.config.mjs",
		"./expo": "./expo.eslint.config.mjs",
		"./prettier": "./prettier.config.mjs"
	},
	"files": [
		"ts.eslint.config.mjs",
		"web.eslint.config.mjs",
		"expo.eslint.config.mjs",
		"prettier.config.mjs"
	],
	"scripts": {
		"fix": "bun lint:fix && bun format",
		"format": "prettier --write . --log-level warn --cache --cache-location node_modules/.cache/prettier/.prettier-cache",
		"format:check": "prettier --check . --log-level warn --cache --cache-location node_modules/.cache/prettier/.prettier-cache",
		"lint": "eslint .",
		"lint:fix": "eslint --fix ."
	},
	"dependencies": {
		"@dword-design/eslint-plugin-import-alias": "^6.0.3",
		"@next/eslint-plugin-next": "^16.0.7",
		"@typescript-eslint/eslint-plugin": "^8.48.1",
		"@typescript-eslint/parser": "^8.48.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-better-tailwindcss": "^3.8.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-native": "^5.0.0",
		"eslint-plugin-unused-imports": "^4.3.0",
		"minimatch": "^10.1.1",
		"prettier-plugin-packagejson": "^2.5.20"
	},
	"devDependencies": {
		"eslint": "^9.18.0",
		"prettier": "^3.7.4",
		"typescript": "^5.7.3"
	},
	"peerDependencies": {
		"eslint": "^9.0.0",
		"prettier": "^3.0.0",
		"typescript": "^5.0.0"
	},
	"packageManager": "bun@1.3.5",
	"engines": {
		"bun": "^1.3.0",
		"node": "^20.0.0"
	}
}
